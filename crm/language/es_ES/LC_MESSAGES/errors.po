msgstr "La fecha de finalización debe ser posterior a la fecha de inicio."

msgid "invalidLogin"
msgstr "La combinación de nombre de usuario y contraseña que ingresó no es correcta."

msgid "missingRequiredFields"
msgstr "No incluyó todos los campos obligatorios."

msgid "noAccessAllowed"
msgstr "Lo siento, no tienes permitido ir allí."

msgid "notLoggedIn"
msgstr "Ya no estás conectado a este sitio."

msgid "passwordCorrupted"
msgstr "Su contraseña está dañada. Póngase en contacto con un administrador del sistema para obtener ayuda."

msgid "ldap/unknownUser"
msgstr "Ese nombre de usuario no está en LDAP. No puede configurar la autenticación LDAP si el usuario no está en LDAP."

msgid "actions/unknown"
msgstr "Esa acción no está en el sistema"

msgid "addresses/unknown"
msgstr "Esa dirección no está en el sistema"

msgid "bookmarks/unknown"
msgstr "Esa búsqueda guardada no está en el sistema"

msgid "categories/unknown"
msgstr "Esa categoría no está en el sistema"

msgid "categories/missingName"
msgstr "Debe proporcionar un nombre"

msgid "categories/missingGroup"
msgstr "Debe proporcionar un grupo"

msgid "categories/missingDepartment"
msgstr "Debe proporcionar un departamento"

msgid "categoryGroups/unknown"
msgstr "Ese grupo no está en el sistema"

msgid "clients/unknownApiKey"
msgstr "Debe proporcionar una api_key válida"

msgid "clients/unknown"
msgstr "Ese cliente no está en el sistema"

msgid "contactMethods/unknown"
msgstr "Ese método de contacto no está en el sistema"

msgid "departments/unknown"
msgstr "Ese departamento no está en el sistema"

