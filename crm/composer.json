{
    "repositories": [{
        "type": "vcs",
        "url": "https://github.com/City-of-Bloomington/blossom-lib"
    }],
    "require": {
        "php": ">=7.2",
        "auth0/auth0-php": "~5",
        "City-of-Bloomington/blossom-lib": "dev-master",
        "graylog2/gelf-php": "@stable",
        "jasig/phpcas": "~1",
        "reprovinci/solr-php-client": "~1",
        "zendframework/zend-db": "@stable",
        "zendframework/zend-log": "@stable",
        "zendframework/zend-loader": "@stable",
        "zendframework/zend-paginator": "@stable",
        "zendframework/zend-permissions-acl": "@stable",
        "zendframework/zend-servicemanager": "@stable"
    },
    "require-dev": {
        "phpunit/phpunit": "@stable",
        "facebook/webdriver": "@stable"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "src/Application"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "src/Test"
        }
    }
}
