{
    "repositories": [{
        "type": "vcs",
        "url": "https://github.com/City-of-Bloomington/blossom-lib"
    }],
    "require": {
        "php": ">=7.2",
        "auth0/auth0-php": "~5",
        "city-of-bloomington/blossom-lib": "dev-master",
        "graylog2/gelf-php":               "@stable",
        "apereo/phpcas":                   "@stable",
        "reprovinci/solr-php-client": "~1",
        "phpmailer/phpmailer":             "@stable",
        "laminas/laminas-db":              "@stable",
        "laminas/laminas-log":             "@stable",
        "laminas/laminas-paginator":       "@stable",
        "laminas/laminas-permissions-acl": "@stable"
    },
    "require-dev": {
        "phpunit/phpunit": "@stable"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "src/Application",
            "Domain\\":      "src/Domain"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "src/Test"
        }
    }
}
